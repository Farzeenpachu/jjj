<body>
    <div class="screen-overlay"></div>



    <section class="content-main">
        <div class="content-header">
            <div>
                <h2 class="content-title card-title">Order List </h2>
                <p>Lorem ipsum dolor sit amet.</p>
            </div>
            <div>
                <a href="/admin/banner"> <button class="btn btn-md rounded font-sm " >Add Banner </button></a>
            </div>
        </div>
        <div class="card mb-4">
            <header class="card-header">
                <div class="row gx-3">
                    <div class="col-lg-4 col-md-6 me-auto">
                        
                    </div>
                    <div class="col-lg-2 col-6 col-md-3">
                        <select class="form-select">
                            <option>Status</option>
                            <option>Active</option>
                            <option>Disabled</option>
                            <option>Show all</option>
                        </select>
                    </div>
                    <div class="col-lg-2 col-6 col-md-3">
                        <select class="form-select">
                            <option>Show 20</option>
                            <option>Show 30</option>
                            <option>Show 40</option>
                        </select>
                    </div>
                </div>
            </header> <!-- card-header end// -->
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th>#ID</th>
                                <th scope="col">Name</th>
                                <th scope="col">Email</th>
                                <th scope="col">Total</th>
                                <th scope="col">Status</th>
                                <th scope="col">Date</th>
                                <th scope="col" class="text-end"> Action </th>
                            </tr>
                        </thead>


                        <tbody>
                            <% banners.forEach((banner,index)=>{%>
                                <tr>
                                    <td>
                                        <%= index+1 %>
                                    </td>
                                    <td><b>
                                            <%= banner.name %>
                                        </b></td>
                                    <td><template class="__cf_email__"
                                            data-cfemail="aec3cfdcd8c7c0eecbd6cfc3dec2cb80cdc1c3">[email&#160;protected]</template>
                                    </td>
                                    <td>$9.00</td>
                                    <td>
                                        <% if(banner.status){%>
                                            <span class="badge rounded-pill alert-success">Active</span>
                                    
                                    <%}else{%>
                                        <span class="badge rounded-pill alert-danger">Blocked</span>
                                
                                <%}%>
                                    </td>
                                    <td>03.12.2020</td>
                                    <td class="text-end">
                                        <a href="#" class="btn btn-md-sucess btn-success rounded font-sm">EDIT</a>
                                        <% if(banner.status){%>
                                            <a href="/admin/bannerstatus/<%= banner._id %>"
                                                class="btn btn-md-danger btn-danger rounded font-sm">BLOCK</a>
                                            <%}else{%>
                                                <a href="/admin/bannerstatus/<%= banner._id %>"
                                                    class="btn btn-md rounded font-sm">unblock</a>
                                                <%}%>
                                    </td>
                                    </tr>
                                    <%})%>
                        </tbody>
                    </table>
                </div> <!-- table-responsive //end -->
            </div> <!-- card-body end// -->
        </div> <!-- card end// -->
