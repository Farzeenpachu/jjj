<body>
    <div class="screen-overlay"></div>

    <section class="content-main">
        <div class="row">
            <form method="post" action="/admin/banner" method="post" enctype="multipart/form-data" >
                <div class="col-9">
                    <div class="content-header">
                        <h2 class="content-title">Add Banner</h2>

                        <div>
                            <button class="btn btn-md rounded font-sm hover-up" type="submit">Publich</button>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="card mb-4">
                        <div class="card-header">
                            <h4>Basic</h4>
                        </div>
                        <div class="card-body">

                            <div class="mb-4">
                                <label for="" class="form-label">Banner Name</label>
                                <input type="text" placeholder="Type here" name="name" class="form-control"
                                    id="">
                            </div>
                            <div class="mb-4">
                                <label for="" class="form-label">Banner Heading</label>
                                <input type="text" placeholder="Type here" name="heading1" class="form-control"
                                    id="">
                            </div>
                            <div class="mb-4">
                                <label for="" class="form-label">Banner Heading2</label>
                                <input type="text" placeholder="Type here" name="heading" class="form-control"
                                    id="">
                            </div>
                            <div class="mb-4">
                                <label for="" class="form-label">Banner description</label>
                                <input type="text" placeholder="Type here" class="form-control" name="discription"
                                    id="">
                            </div>


                        </div>
                    </div> <!-- card end// -->
                </div>
                <div class="col-lg-3">
                    <div class="card mb-4">
                        <div class="card-header">
                            <h4>Media</h4>
                        </div>
                        <div class="card-body">
                            <div class="input-upload">
                                <img src="/assets1/imgs/theme/upload.svg" alt="">
                                <input class="form-control" name="image" type="file">
                            </div>
                        </div>

                    </div> <!-- card end// -->
                </div>
            </form>
        </div>
    </section> <!-- content-main end// -->



</body>



