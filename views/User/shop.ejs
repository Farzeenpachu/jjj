<script>
    $(document).ready(function () {
        $('#search').on('keyup', function () {
            var searchValue = $(this).val();
            console.log(searchValue);
            if (searchValue !== '') {
                $.ajax({
                    type: 'post',
                    url: '/search/' + searchValue,
                    success: function (data) {
                        console.log(data);
                        $('#search-results').empty()
                        $.each(data, function (index, value) {
                            $('#search-results').append(`<a style=" text-decoration: none; color: inherit"
                         href="/singleproduct/${value._id}"><li>` + value.productTitle + '</li></a>');
                        });
                    }
                });
            } else {
                $('#search-results').empty();
            }
        });
    });
</script>
<script>
        function addToWishlist(proId,user) {
        $.ajax({
            url: /add-wishlist/ + proId,
            method: 'get',
            success: (res) => {
                if (res.status) {
                    $('#wishlist-count').html(res.wcount)
                    toastr.success('Item added to wishlist!');
                } else {
                    if (!user) {
                        toastr.warning('You have to login first')
                    } else {
                        toastr.warning('Sorry, This Item is Out Of Stock.')
                    }
                }
            }
        })
    }
</script>
<script>
    function addToCart(proId, user) {
        $.ajax({
            url: /add-to-cart/ + proId,
            method: 'get',

            success: (res) => {
                if (res.status) {

                    $('#cart-count').html(res.cartcount)
                    toastr.success('Item added to cart!');
                } else {
                    if (!user) {
                        toastr.warning('You have to login first')
                    } else {
                        toastr.warning('Sorry, This Item is Out Of Stock.')
                    }
                }
            }
        })
        toastr.options = {
            //   "closeButton": true,
            "debug": false,
            "newestOnTop": false,
            //   "progressBar": true,
            "positionClass": "toast-top-right",
            // "preventDuplicates": true,
            "onclick": null,
            "showDuration": "300",
            "hideDuration": "1000",
            "timeOut": "2000",
            "extendedTimeOut": "1000",
            "showEasing": "swing",
            "hideEasing": "linear",
            "showMethod": "show",
            "hideMethod": "hide"
        }
    }

</script>